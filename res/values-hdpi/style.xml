    <!-- 个人中心home界面 -->
    <style name="private_zoo_home_text_item" parent="@style/single_padding">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/guishudi_black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">@dimen/gd_person_item_title_text_size</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingTop">12dp</item>
    </style>
    <!-- 个人中心条目信息 -->
    <style name="private_zoo_text_item">
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">@color/guishudi_black</item>
        <item name="android:textSize">@dimen/gd_person_item_title_text_size</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <!-- 个人中心条目信息(右) -->
    <style name="private_zoo_text_item_right">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">@color/gd_setting_text</item>
        <item name="android:textSize">@dimen/gd_person_item_title_text_size</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <!-- 首页字体大小 -->
    <style name="gd_home_item_big_font">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/gd_home_item_big_font</item>
    </style>
    <!-- 首页字体大小 -->
    <style name="gd_home_item_small_font">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">@dimen/gd_home_item_small_font</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>
    <!-- 首页ITEM大小 -->
    <style name="gd_home_item">
        <item name="android:paddingLeft">@dimen/gd_homeitem_pad_left</item>
        <item name="android:paddingRight">@dimen/gd_homeitem_pad_right</item>
        <item name="android:paddingTop">@dimen/gd_homeitem_pad_top</item>
        <item name="android:paddingBottom">@dimen/gd_homeitem_pad_bottom</item>
    </style>
    <!-- 查询 按钮 -->
    <style name="mobile_query_btn">
        <item name="android:layout_width">80dip</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:background">@drawable/private_zoo_mobile_query_seletor</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">22sp</item>
        <item name="android:text">查  询</item>
        <item name="android:layout_marginRight">15dip</item>
    </style>
    <!-- 快捷输入 -->
    <style name="gd_sms_quickinput_item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/sms_detail_qucickinput_hei</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/sms_detail_qucickinput_item_marginleft</item>
        <item name="android:layout_marginRight">@dimen/sms_detail_qucickinput_item_marginleft</item>
        <item name="android:paddingLeft">@dimen/sms_detail_qucickinput_item_padleft</item>
        <item name="android:paddingRight">@dimen/sms_detail_qucickinput_item_padleft</item>
        <item name="android:layout_marginTop">@dimen/sms_detail_qucickinput_item_top</item>
        <item name="android:textColor">#1b1b1b</item>
        <item name="android:textSize">@dimen/fontsize_28</item>
    </style>

    <style name="guishudi_lable">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <!-- 查询按钮 -->
    <style name="query_button">
        <item name="android:textSize">@dimen/fontsize_28</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <!-- 开关按钮 -->
    <style name="switch_button">
        <item name="android:gravity">right</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@drawable/setting_switch_on</item>
    </style>
    <!-- 归属地文本框黑色字体 -->
    <style name="guishudi_balck">
        <item name="android:textColor">@color/guishudi_black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">10dp</item>
    </style>
    <!-- 归属地查询(大标题) -->
    <style name="guishudi">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/guishudi_black</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">20dp</item>
    </style>
    <!-- 说明文字 -->
    <style name="subdescription_text">
          <item name="android:textSize">11sp</item>
